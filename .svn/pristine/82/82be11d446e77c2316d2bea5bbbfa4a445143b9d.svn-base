            transform: scaleY(0.92) rotateX(90deg) skewX(10deg); }
  100% {
    -webkit-transform: scaleY(0.92) rotateX(181deg) skewX(0deg);
            transform: scaleY(0.92) rotateX(181deg) skewX(0deg); } }


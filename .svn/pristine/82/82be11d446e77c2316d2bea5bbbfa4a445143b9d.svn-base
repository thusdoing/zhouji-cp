.flip-now {
  -webkit-transform-origin: center bottom 0;
          transform-origin: center bottom 0;
  -webkit-animation: flip-clock 1s 800ms infinite linear both;
          animation: flip-clock 1s 800ms infinite linear both; }

.flip-now-next {
  -webkit-transform-origin: center top 0;
          transform-origin: center top 0;
  -webkit-animation: flip-clock-next 1s 800ms infinite linear both;
          animation: flip-clock-next 1s 800ms infinite linear both; }


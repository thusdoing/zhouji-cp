    A set of useful LESS mixins
    More info at: http://lesselements.com
  ---------------------------------------------------*/
@media screen and (max-width: 992px) {
  .top-section {
    height: 50vh;
  }
  .top-section .w3-center img {
    width: 90px;
    height: 40px;
  }
  .top-section .w3-center h2 {
    font-size: 15px;
    line-height: 21px;
  }
  .top-section .w3-center h1 {
    font-size: 36px;
    line-height: 50px;
    margin: 16px 0;
  }
  .top-section .w3-center:after {
    width: 60px;
    height: 3px;
    margin: 0 auto;
  }
  .group-section {
    padding: 100px 0 0;
  }
  .group-section h2 {
    font-size: 30px;
    line-height: 42px;
    margin-bottom: 30px;
  }
  .group-section h2:after {
    width: 60px;
    height: 3px;
    margin: 24px auto 0;
  }
  .group-section p {
    font-size: 16px;
    line-height: 22px;
    text-align: center;
    padding: 0 60px;
  }
  .group-section > img {
    width: 240px;
    height: 240px;
    object-fit: contain;
    -o-object-fit: contain;
    margin-bottom: 80px;
  }
  .group-section ul {
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }
  .group-section ul li {
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    margin: 0 45px 60px;
    position: relative;
  }
  .group-section ul li img {
    width: 60px;
    height: 60px;
    margin-bottom: 24px;
  }
  .group-section ul li h3 {
    font-size: 18px;
    text-align: center;
    color: #ffffff;
  }
  .team-section {
    padding: 100px 0 70px;
  }
  .team-section h2 {
    font-size: 30px;
    line-height: 42px;
    margin-bottom: 30px;
  }
  .team-section h2:after {
    width: 60px;
    height: 3px;
    margin: 24px auto 0;
  }
  .team-section p {
    font-size: 16px;
    line-height: 22px;
    text-align: center;
    padding: 0 60px;
  }
  .team-section div img {
    width: 130px;
    height: 130px;
    object-fit: contain;
    -o-object-fit: contain;
    margin: 0 24px;
  }
  .join-section {
    padding: 100px 0 70px;
  }
  .join-section h2 {
    font-size: 30px;
    line-height: 42px;
    margin-bottom: 30px;
  }
  .join-section h2:after {
    width: 60px;
    height: 3px;
    margin: 24px auto 0;
  }
  .join-section p {
    font-size: 16px;
    line-height: 22px;
    text-align: center;
    padding: 0 60px;
  }
}


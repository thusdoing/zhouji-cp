}

div.modal .modal-content .drawResult .ball .animate-ball,
div.zone.last-draw-result .drawResult .ball .animate-ball {
  position: relative;
  z-index: 1;
  color: #e60b0b;
  font-weight: 700;
  width: 40px;
  text-align: center;
  overflow: hidden;
  margin: 0 auto;
}

div.modal .modal-content .drawResult .ball .animate-ball>.inner,
div.zone.last-draw-result .drawResult .ball .animate-ball>.inner {
  border-radius: 40px;
  font-weight: 300;
  width: 40px;
  height: 35px;
  overflow-y: hidden;
  margin-right: -20px;
  padding-right: 5px;
}

header div.zone.last-draw-result .link {
  margin-top: 2px;
  padding-right: 10px;
  text-align: right;
}

header div.zone.last-draw-result .link a {
  font-size: 12px;
  color: #9b9b9b;
  text-decoration: none;
  padding: 0 5px;
  position: relative;
  display: inline;
}

header div.zone.last-draw-result .link a:first-child:after {
  content: '';
  display: block;
  position: absolute;
  width: 1px;
  height: 80%;
  background: #9b9b9b;
  right: -1px;
  top: 3px;
}

header div.zone.last-draw-result .link a:hover {
  color: #d0011b;
}

// 倒计时
header {
  /deep/.flip-clock-wrapper ul {
    width: 40px;
    height: 60px;
  }
  /deep/.flip-clock-wrapper ul li a div.up:after {
    top: 35px;
  }
  /deep/.flip-clock-wrapper ul li {
    line-height: 60px;
  }
  /deep/.flip-clock-wrapper ul li a div div.inn {
    font-size: 50px;
  }
  /deep/.flip-clock-dot {
    background: #e4393c;
    width: 5px;
    height: 5px;
    left: 1px;
  }
  /deep/.flip-clock-dot.top {
    top: 38px;
  }
  /deep/.flip-clock-dot.bottom {
    bottom: -13px;
  }
  /deep/.swiper-button-disabled {
    display: none;
  }
  /deep/.flip-clock-wrapper ul li a div div.inn {
    background-color: #333;
  }
}

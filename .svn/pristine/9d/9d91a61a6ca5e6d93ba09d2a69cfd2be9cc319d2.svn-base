div.zone.last-draw-result .drawResult {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  min-height: 3em;
  padding: 0 10px;
}

div.modal .modal-content .drawResult .ball,
div.zone.last-draw-result .drawResult .ball {
  position: relative;
  -webkit-box-flex: 0;
  -ms-flex: 0 0 auto;
  flex: 0 0 auto;
  width: 40px;
  height: 40px;
  -webkit-box-shadow: 0 0 0 2px #cfcfcf, 0 2px 2px #dbdbdb;
  box-shadow: 0 0 0 2px #cfcfcf, 0 2px 2px #dbdbdb;
  margin: 0 10px;
  border-radius: 40px;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#ffffff), color-stop(60%, #f5f5f5), to(#dbdbdb)), -webkit-gradient(linear, left top, left bottom, from(#ff5e3f), to(#ea0f0f));
  background-image: linear-gradient(to bottom, #ffffff 0%, #f5f5f5 60%, #dbdbdb 100%), linear-gradient(to bottom, #ff5e3f, #ea0f0f);
  background-origin: border-box;
  background-clip: content-box, border-box;
  border: 2px solid transparent;
  color: #fff;
  font-size: 24px;
  line-height: 35px;
  display: inline-block;
}

div.modal .modal-content .drawResult .ball.pcddgreen,
div.zone.last-draw-result .drawResult .ball.pcddgreen{
  background-image: linear-gradient(rgb(174, 240, 83), rgb(15, 144, 21)), linear-gradient(rgb(252, 252, 252), rgb(219, 219, 219));
}
div.modal .modal-content .drawResult .ball.pcddblue,
div.zone.last-draw-result .drawResult .ball.pcddblue{

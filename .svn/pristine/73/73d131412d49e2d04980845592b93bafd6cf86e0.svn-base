.top-section {
  display: flex;
  align-items: center;
  height: 86vh;
  padding: 160px 0;
  position: relative;
}
.top-section .kv {
  width: 100%;
  height: 100%;
  object-fit: cover;
  -o-object-fit: cover;
}
.top-section .w3-center {
  width: 100%;
  padding: 24px;
  position: absolute;
  top: 29%;
}
.top-section .w3-center img {
  width: 180px;
  height: 80px;
}
.top-section .w3-center h2 {
  font-size: 30px;
  line-height: 42px;
}
.top-section .w3-center h1 {
  font-size: 72px;
  font-weight: normal;
  line-height: 100px;
  margin: 12px 0 24px;
}
.top-section .w3-center:after {
  content: "";
  display: block;
  width: 90px;
  height: 6px;
  background: #d41879;
  background: -webkit-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: -o-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: -moz-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  margin: 0 auto;
}
.wordtest{
}
h10{
    font-size: 18px;
    font-weight: 400;
    line-height: 1;
}
h11{
    font-size: 16px;
    font-weight: 400;
    line-height: 1;
}

h12{
    font-size: 12px;
    font-weight: 400;
    line-height: 1;
}

.newstitle{
    margin: 0px auto 0px auto;
}
.newsline{
    margin-top: 10px;
    margin-bottom: 10px;
    height: 2px;
    background: radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
}

.section{
    padding-top: 50px;
    padding-bottom: 50px;
    color: #444;
    background-color: #fff;
}


.news{   
    border: 1px solid #fff;
    transition: .5s;
    border-radius: 5px;
    background-color:#fff;
    padding: 10px 10px 15px 10px;
    margin: 20px 0px 20px 0px;
    display: flex;
}

.news:hover{
    border: 1px solid #cccccc;
}


.imginfo{
margin-right: 10px;  
}

.smallimg{
    transition: .5s;
    height: 120px;
    width: 200px;
}
.smallimg:hover{
-webkit-filter:brightness(.7);
}

.bigimg{
background-size: cover;
}
.newsinfo{
    width: 100%;
}

.nameblock{
    margin-bottom: 10px;
}

.artblock{
     margin-bottom: 10px;
}
.artblock .font{
    color: #606060;
}

.noteblock{
    width: 100%;
}

.noteblock_box{
    font-size: 14px;
    color: #8c8c8c;
    width: 100%;
}
.noteblock .font{
    font-size: 12px;
    color: #8c8c8c;
    line-height: 50px;
}


.botton{
    font-weight: 300;
    transition: 0.5s;
    cursor: pointer;
    color: #444;
    background-color: #fff;
    float: right;
    line-height: 37px;
    height: 50px;
    width: 80px;
    border: 1px solid #444;
    text-align: center;
}
.botton:hover{
    
    cursor: pointer;
    color: #fff;
    background-color: #444;
}




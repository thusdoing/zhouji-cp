    #main{margin: 0}
    .order_box{z-index:999}
    .order_box h4{height:35px; line-height:40px; font-size: 1.35rem;}
    .order_text li{ line-height: 2rem; }
    .order_text li span{width:100px; font-size: 1.3rem;}
	.order_text li p{ padding:0 0 0 100px;font-size: 1.3rem;}
	.up{background-size:30%;}
	/*	.bank_list h4,.quick_title2,.already_quick h4,.already_quick h4 span{font-size:1.6rem; line-height: 1.5rem;}*/
	.bank_list ul li{width:33.3%;}
	.bank_list ul li .upp{font-size: 1.3rem;} 
	.button button{ height:3.5rem; line-height: 3.5rem}
	.faq li h6{ font-size: 1.3rem; line-height: 1.8rem; }
	.faq li p{ font-size: 1.1rem;  line-height: 1.5rem; padding-top: 0.5rem;}
	.successful figure figcaption{  line-height: 3rem;}
	.successful ul li p{padding:0 0 0 105px;font-size: 1.4rem;}
	.successful ul li span{font-size: 1.4rem;}
	.return{ font-size: 1.4rem; line-height: 1.6rem}
	.error ul li p{ padding:0 0 0 60px; font-size: 1.3rem}
	.error ul li span{width:70px; font-size: 1.2rem;}
	.error .return{ margin-top:1rem;}
	.credit_new_bottom span{ line-height:2rem;font-size: 1.2rem;}
	.new_card_btn{ height:40px; line-height: 40px; }
	.new_card_btn span{ height:39px; }
	.new_card_main { height:250px;  }	
	.new_card_submit button,.new_card_submit1 button,.bank_ico a{font-size: 1.4rem; }
	.ew_card_up{ font-size: 1rem;color:#35c295;display:block;margin-top:.7rem;}
	.agreement_main p{  font-size: 1.2rem; line-height: 1.5rem;}
	.agreement_main h4{  font-size: 1.3rem; }
	.already_quick ul li span{ font-size: 1.3rem; }
	.new_card_main ul li {  width:50%;}
	.new_card_submit,.agreement_submit,.new_card_submit1{height:45px;}
	.new_card_submit button,.agreement_submit button,.new_card_submit1 button{height:30px; line-height: 30px; font-size: 1.4rem;}
	.new_quick{ font-size: 1.3rem;}	#agreement_btn{text-align:center; }
	.credit_new_bottom .warning{ font-size:1.2rem;}
	.new_card_top,.agreement_top,.agreement_top2{ font-size: 1.5rem; height:45px;  line-height: 45px;}
	.faq_title{line-height: 45px ;font-size: 1.6rem; text-indent: 0rem;}
	.service h5{ line-height: 1.7rem; font-size: 1.5rem;  }
	.service p{ line-height: 1.5rem; font-size: 1.3rem;  }
	#chPhoneNumber{ position:static;  padding: 0; margin:0 0 0 72px; line-height: 2.5rem}


}
@media only screen and (max-width:500px){
	

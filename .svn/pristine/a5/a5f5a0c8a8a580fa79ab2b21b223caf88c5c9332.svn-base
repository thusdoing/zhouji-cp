	.validation span,.credit li .input_box input,.validation div input,.areaCodeStyle{ height:3rem; line-height:3rem;}
	.access{ line-height: 3rem; font-size: 1.2rem; }
	.credit li .input_box,.credit_new_bottom,.validation div{ padding: 0 0 0 90px;}

	html{ font-size:11px;}

/*---------------------------------------------------
    LESS Elements 0.9
  ---------------------------------------------------
    A set of useful LESS mixins
    More info at: http://lesselements.com
  ---------------------------------------------------*/
@media screen and (max-width: 992px) {
  .top-section {
    height: 50vh;
  }
  .top-section .w3-center img {
    width: 90px;
    height: 40px;
  }
  .top-section .w3-center h2 {
    font-size: 15px;
    line-height: 21px;
  }
  .top-section .w3-center h1 {
    font-size: 36px;
    line-height: 50px;
    margin: 16px 0;
  }
  .top-section .w3-center:after {
    width: 60px;
    height: 3px;
    margin: 0 auto;
  }
  .second-section {
    padding: 100px 0 120px;
  }
  .second-section .w3-center .w3-row-padding .w3-col {
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }
  .second-section .w3-center .w3-row-padding .w3-col .icon {
    margin: 0;
  }
  .second-section .w3-center .w3-row-padding .w3-col h2 {
      
    font-size: 24px;
    line-height: 33px;
    margin-bottom: 24px;
  }
  .second-section .w3-center .w3-row-padding .w3-col ul,
  .second-section .w3-center .w3-row-padding .w3-col p {
    font-size: 16px;
    max-width: 500px;
  }
  .second-section .w3-center .w3-row-padding .w3-col ul {
    padding-left: 20px;
  }
  .second-section .w3-center .w3-row-padding .w3-col + .w3-col {
    margin-top: 80px;
  }
  .partner-section {
    padding: 100px 24px 120px;
  }
  .partner-section .w3-center .w3-row-padding .w3-col {
    padding: 0 6px;
    margin-bottom: 36px;
  }
  .partner-section .w3-center h2 {
    font-size: 30px;
    line-height: 42px;
    margin-bottom: 60px;
  }
  .partner-section .w3-center h2:after {
    width: 60px;
    height: 3px;
    margin-top: 16px;
  }
  .solution-section {
    padding: 100px 0 70px;
  }
  .solution-section h2 {
    font-size: 30px;
    line-height: 42px;
    margin-bottom: 30px;
  }
  .solution-section h2:after {
    width: 60px;
    height: 3px;
    margin: 16px auto;
  }
  .solution-section p {
    padding: 0 60px;
    font-size: 16px;
    line-height: 22px;
    margin-bottom: 130px;
    text-align: left;
  }
  .solution-section > img {
    padding: 0 90px;
  }
  .solution-section .w3-row-padding {
    padding: 0 36px;
  }
  .solution-section .w3-row-padding .w3-col {
    padding: 0 24px;
    height: 265px;
    margin-bottom: 50px;
  }
  .solution-section .w3-row-padding .w3-col h3 {
    font-size: 18px;
    line-height: 25px;
    margin-top: 24px;
  }
  .solution-section .w3-row-padding .w3-col p,
  .solution-section .w3-row-padding .w3-col ul {
    font-size: 14px;
    line-height: 20px;
  }
}

/*color-------------------------------------------------------------------------------------*/
/*---------------------------------------------------
    LESS Elements 0.9
  ---------------------------------------------------
    A set of useful LESS mixins
    More info at: http://lesselements.com
  ---------------------------------------------------*/
@media screen and (max-width: 600px) {
  .top-section {
    height: 86vh;
  }
  .top-section .w3-center {
    text-align: left !important;
  }
  .top-section .w3-center img {
    width: 90px;
    height: 40px;
  }
  .top-section .w3-center h2 {
    font-size: 15px;
    line-height: 21px;
  }
  .top-section .w3-center h1 {
    font-size: 36px;
    line-height: 50px;
    margin: 16px 0;
  }
  .top-section .w3-center:after {
    width: 60px;
    height: 3px;
    margin: 0;
  }
  .second-section .w3-center .w3-row-padding .w3-col {
    display: block;
    text-align: left;
  }
  .second-section .w3-center .w3-row-padding .w3-col + .w3-col {
    margin-top: 80px;
  }
  .partner-section .w3-center h2 {
    text-align: left;
  }
  .partner-section .w3-center h2:after {
    margin-left: 0;
  }
  .partner-section .w3-center .w3-row-padding .w3-col {
    padding: 0 20px;
    height: 50px;
  }
  .partner-section .w3-center .w3-row-padding .w3-col img {
    width: 100%;
    max-width: 100%;
    height: 100%;
    object-fit: contain;
    -o-object-fit: contain;
  }
  .solution-section {
    padding: 100px 0 0;
  }
  .solution-section h2 {
    text-align: left;
    padding: 0 24px;
  }
  .solution-section h2:after {
    width: 60px;
    height: 3px;
    margin: 16px 0 0;
  }
  .solution-section p {
    padding: 0 24px;
  }
  .solution-section > img {
    padding: 0 24px;
  }
  .solution-section .w3-row-padding {
    padding: 0;
  }
  .solution-section .w3-row-padding .w3-col {
    height: initial;
  }
  .solution-section .w3-row-padding .w3-col p,
  .solution-section .w3-row-padding .w3-col ul {
    max-width: 500px;
  }
  .solution-section .circle1 {
    top: 19%;
    right: 72%;
    width: 318px;
    height: 318px;
    -webkit-box-shadow: -15px 15px 30px rgba(0, 0, 0, 0.3);
    -moz-box-shadow: -15px 15px 30px rgba(0, 0, 0, 0.3);
    box-shadow: -15px 15px 30px rgba(0, 0, 0, 0.3);
  }
  .solution-section .circle2 {
    top: 11%;
    left: 61%;
  }
  .solution-section .circle2 img {
    width: 155px;
    height: 283px;
  }
  .ai-section .circle1 {
    top: 14%;
    left: 60%;
    right: initial;
    width: 400px;
    height: 400px;
  }
  .ai-section .circle2 {
    top: 11%;
    right: 68%;
    left: initial;
  }
  .ai-section .circle2 img {
    width: 132px;
    height: 296px;
  }
}
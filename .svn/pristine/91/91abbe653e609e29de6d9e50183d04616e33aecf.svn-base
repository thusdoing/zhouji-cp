/*---------------------------------------------------
    LESS Elements 0.9
  ---------------------------------------------------
    A set of useful LESS mixins
    More info at: http://lesselements.com
  ---------------------------------------------------*/
.top-section {
  display: flex;
  align-items: center;
  height: 86vh;
  padding: 160px 0;
  position: relative;
}
.top-section .kv {
  width: 100%;
  height: 100%;
  object-fit: cover;
  -o-object-fit: cover;
}
.top-section .w3-center {
  width: 100%;
  padding: 24px;
  padding-right: 0;
  position: absolute;
  top: 29%;
}
.top-section .w3-center img {
  width: 180px;
  height: 80px;
}
.top-section .w3-center h2 {
  font-size: 30px;
  line-height: 42px;
}
.top-section .w3-center h1 {
  font-size: 72px;
  font-weight: normal;
  line-height: 100px;
  margin: 12px 0 24px;
}

.second-section {
  display: flex;
  align-items: center;
  padding: 60px 0;
  transform: translate(0, 0);
}
.second-section .w3-center {
  width: 100%;
  max-width: 1020px;
  margin: 0 auto;
}
.second-section .w3-center .w3-row-padding {
  padding: 0;
}
.second-section .w3-center .w3-row-padding .w3-col {
  padding: 0 20px;
}
.second-section .w3-center .w3-row-padding .w3-col .icon {
  position: relative;
  width: 86px;
  height: 86px;
  background: #000;
  background-clip: padding-box;
  border-radius: 50%;
  border: 2px solid transparent;
  margin: 0 auto;
}
.second-section .w3-center .w3-row-padding .w3-col .icon:after {
  position: absolute;
  content: '';
  z-index: -1;
  border-radius: 50%;
  top: -2px;
  bottom: -2px;
  left: -2px;
  right: -2px;
  background: #f8eea4;
  background: -webkit-radial-gradient(circle at 92% 10%, #f8eea4, #b9760b);
  background: -o-radial-gradient(circle at 92% 10%, #f8eea4, #b9760b);
  background: -moz-radial-gradient(circle at 92% 10%, #f8eea4, #b9760b);
  background: radial-gradient(circle at 92% 10%, #f8eea4, #b9760b);
}
.second-section .w3-center .w3-row-padding .w3-col .icon img {
  width: 50px;
  height: 50px;
  object-fit: contain;
  -o-object-fit: contain;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.second-section .w3-center .w3-row-padding .w3-col h2 {
    
  font-size: 30px !important;
  font-weight: normal;
  line-height: 50px;
  margin: 48px 0;
}
.second-section .w3-center .w3-row-padding .w3-col p,
.second-section .w3-center .w3-row-padding .w3-col ul {
  text-align: left;
  font-size: 24px;
  font-weight: normal;
}
.second-section .w3-center .w3-row-padding .w3-col ul {
  padding-left: 25px;
}
.partner-section {
  display: flex;
  align-items: center;
  padding: 60px 0;
  background: #f8f8f8;
  background: -webkit-radial-gradient(circle at 92% 10%, #f8f8f8, #e7e7e7);
  background: -o-radial-gradient(circle at 92% 10%, #f8f8f8, #e7e7e7);
  background: -moz-radial-gradient(circle at 92% 10%, #f8f8f8, #e7e7e7);
  background: radial-gradient(circle at 92% 10%, #f8f8f8, #e7e7e7);
}
.partner-section .w3-center {
  width: 100%;
  max-width: 1020px;
  margin: 0 auto;
}
.partner-section .w3-center .w3-row-padding {
  padding: 0;
}
.partner-section .w3-center .w3-row-padding .w3-col {
  padding: 0 12px;
  margin-bottom: 24px;
}
.partner-section .w3-center .w3-row-padding .w3-col img {
  width: 100%;
  max-width: 305px;
  object-fit: contain;
  -o-object-fit: contain;
}
.partner-section .w3-center h2 {
  font-size: 60px;
  font-weight: normal;
  line-height: 84px;
  margin-bottom: 50px;
  color: #000;
}
.partner-section .w3-center h2:after {
  content: "";
  display: block;
  width: 90px;
  height: 6px;
  background: #d41879;
  background: -webkit-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: -o-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: -moz-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  margin: 24px auto 0;
}
.solution-section {
  position: relative;
  padding: 60px 0 0;
  background: #fff;
  overflow: hidden;
}
.solution-section h2 {
  position: relative;
  font-size: 60px;
  font-weight: normal;
  line-height: 84px;
  margin-bottom: 60px;
  color: #000;
  padding: 0 60px;
}
.solution-section h2:after {
  content: "";
  display: block;
  width: 90px;
  height: 6px;
  background: #d41879;
  background: -webkit-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: -o-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: -moz-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  margin: 24px auto 0;
}
.solution-section p {
  position: relative;
  font-size: 26px;
  line-height: 37px;
  font-weight: normal;
  color: #000;
  max-width: 1000px;
  margin: 0 auto 100px;
}
.solution-section > img {
  padding: 0 130px;
  position: relative;
  width: 100%;
  max-width: 1314px;
  object-fit: contain;
  -o-object-fit: contain;
}
.solution-section .w3-row-padding {
  position: relative;
  width: 100%;
  max-width: 1020px;
  padding: 0;
  margin: 0 auto 0;
}
.solution-section .w3-row-padding .w3-col {
  padding: 0 56px;

  height: 350px;
}
.solution-section .w3-row-padding .w3-col img {
  width: 60px;
  height: 60px;
}
.solution-section .w3-row-padding .w3-col h3 {
  font-size: 36px;
  font-weight: 400 !important;
  line-height: 50px;
  color: #000;
}
.solution-section .w3-row-padding .w3-col p,
.solution-section .w3-row-padding .w3-col ul {
    margin-top: 10px !important;
  color: #000;
  font-weight: normal;
  font-size: 17px;
  line-height: 24px;
  max-width: 373px;
  margin: 0;
  padding: 0;
}
.solution-section .w3-row-padding .w3-col ul {
    margin-top: 10px !important;
  list-style: inside;
}
.solution-section .circle1 {
  position: absolute;
  top: 18%;
  right: 72%;
  width: 914px;
  height: 914px;
  background: #e346eb;
  background: -webkit-radial-gradient(circle at 84% 14%, #e346eb, #0a7cc8);
  background: -o-radial-gradient(circle at 84% 14%, #e346eb, #0a7cc8);
  background: -moz-radial-gradient(circle at 84% 14%, #e346eb, #0a7cc8);
  background: radial-gradient(circle at 84% 14%, #e346eb, #0a7cc8);
  -webkit-box-shadow: -43px 43px 86px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: -43px 43px 86px rgba(0, 0, 0, 0.3);
  box-shadow: -43px 43px 86px rgba(0, 0, 0, 0.3);
  border-radius: 50%;
}
.solution-section .circle2 {
  position: absolute;
  top: 15%;
  left: 78%;
}
.solution-section .circle2 img {
  width: 453px;
  height: 705px;
  object-fit: contain;
  -o-object-fit: contain;
}
.ai-section {
  padding: 60px 0 80px;
  overflow: hidden;
  background: #f8f8f8;
  background: -webkit-radial-gradient(circle at 92% 10%, #f8f8f8, #e7e7e7);
  background: -o-radial-gradient(circle at 92% 10%, #f8f8f8, #e7e7e7);
  background: -moz-radial-gradient(circle at 92% 10%, #f8f8f8, #e7e7e7);
  background: radial-gradient(circle at 92% 10%, #f8f8f8, #e7e7e7);
}
.ai-section .w3-row-padding .w3-col {
  height: 350px;
}
.ai-section .circle1 {
  top: 20%;
  left: 68%;
  right: initial;
}
.ai-section .circle2 {
  top: 8%;
  right: 78%;
  left: initial;
}
.ai-section .circle2 img {
  width: 464px;
  height: 745px;
  object-fit: contain;
  -o-object-fit: contain;
}


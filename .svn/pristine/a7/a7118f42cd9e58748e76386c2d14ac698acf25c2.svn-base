/*color-------------------------------------------------------------------------------------*/
/*---------------------------------------------------
    LESS Elements 0.9
  ---------------------------------------------------
    A set of useful LESS mixins
    More info at: http://lesselements.com
  ---------------------------------------------------*/
.top-section {
  display: flex;
  align-items: center;
  height: 86vh;
  padding: 160px 0;
  position: relative;
}
.top-section .kv {
  width: 100%;
  height: 100%;
  object-fit: cover;
  -o-object-fit: cover;
}
.top-section .w3-center {
  width: 100%;
  padding: 24px;
  position: absolute;
  top: 29%;
}
.top-section .w3-center img {
  width: 180px;
  height: 80px;
}
.top-section .w3-center h2 {
  font-size: 30px;
  line-height: 42px;
}
.top-section .w3-center h1 {
  font-size: 72px;
  font-weight: normal;
  line-height: 100px;
  margin: 12px 0 24px;
}
.top-section .w3-center:after {
  content: "";
  display: block;
  width: 90px;
  height: 6px;
  background: #d41879;
  background: -webkit-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: -o-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: -moz-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  margin: 0 auto;
}
.backend-section {
  position: relative;
  padding: 60px 0 207px;
  background: #fff;
  transform: translate(0, 0);
}
.backend-section .circle1 {
  position: absolute;
  top: 47%;
  right: 80%;
  width: 733px;
  height: 733px;
  background: #00d4e9;
  background: -webkit-radial-gradient(circle at 84% 14%, #00d4e9, #5800bc);
  background: -o-radial-gradient(circle at 84% 14%, #00d4e9, #5800bc);
  background: -moz-radial-gradient(circle at 84% 14%, #00d4e9, #5800bc);
  background: radial-gradient(circle at 84% 14%, #00d4e9, #5800bc);
  -webkit-box-shadow: -43px 43px 86px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: -43px 43px 86px rgba(0, 0, 0, 0.3);
  box-shadow: -43px 43px 86px rgba(0, 0, 0, 0.3);
  border-radius: 50%;
}
.backend-section .circle2 {
  position: absolute;
  top: 16%;
  right: 0;
}
.backend-section .circle2 img {
  width: 442px;
  height: 976px;
  object-fit: contain;
  -o-object-fit: contain;
}
.backend-section h2 {
  position: relative;
  font-size: 60px;
  font-weight: normal;
  line-height: 84px;
  margin-bottom: 60px;
  color: #000;
  padding: 0 24px;
}
.backend-section h2:after {
  content: "";
  display: block;
  width: 90px;
  height: 6px;
  background: #d41879;
  background: -webkit-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: -o-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: -moz-radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  background: radial-gradient(circle at 100% 100%, #d41879, #2a00d7);
  margin: 24px auto 0;
}
.backend-section p {
  position: relative;
  font-size: 26px;
  line-height: 37px;
  color: #000;
  max-width: 1000px;
  margin: 0 auto;
  padding: 0 24px;
}
.backend-section .pic {
  position: relative;
  width: 960px;
  height: 707px;
  object-fit: contain;
  -o-object-fit: contain;
  margin-top: 50px;
}
.backend-section .w3-row-padding {
  position: relative;
  width: 100%;
  max-width: 1020px;
  padding: 0;
  margin: 0 auto;
}
.backend-section .w3-row-padding .w3-col {
  padding: 0 55px;
  margin-top: 80px;
}
.backend-section .w3-row-padding .w3-col .icon {
  position: relative;
  width: 86px;
  height: 86px;
  background: #FFF;
  background-clip: padding-box;
  border-radius: 50%;
  border: 2px solid transparent;
  margin: 0 auto;
}
.backend-section .w3-row-padding .w3-col .icon:after {
  position: absolute;
  content: '';
  z-index: -1;
  border-radius: 50%;
  top: -2px;
  bottom: -2px;
  left: -2px;
  right: -2px;
  background: #d41879;
  background: -webkit-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: -o-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: -moz-radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
  background: radial-gradient(circle at 92% 10%, #d41879, #2a00d7);
}
.backend-section .w3-row-padding .w3-col .icon img {
  width: 60px;
  height: 60px;
  object-fit: contain;
  -o-object-fit: contain;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.backend-section .w3-row-padding .w3-col h3 {
    text-align: center !important;
  font-size: 26px !important;
  font-weight: normal;
  line-height: 50px;
  color: #000;
  margin: 20px 0;
  text-align: center;
}
.backend-section .w3-row-padding .w3-col p,
.backend-section .w3-row-padding .w3-col ul {
  color: #000;
  font-size: 17px;
  line-height: 24px;
  margin: 0;
  padding: 0;
}
.backend-section .w3-row-padding .w3-col ul {
  padding-left: 1.5em;
}

/*color-------------------------------------------------------------------------------------*/
/*---------------------------------------------------
    LESS Elements 0.9
  ---------------------------------------------------
    A set of useful LESS mixins
    More info at: http://lesselements.com
  ---------------------------------------------------*/
@media screen and (max-width: 992px) {
  .top-section {
    height: 50vh;
  }
  .top-section .w3-center img {
    width: 90px;
    height: 40px;
  }
  .top-section .w3-center h2 {
    font-size: 15px;
    line-height: 21px;
  }
  .top-section .w3-center h1 {
    font-size: 36px;
    line-height: 50px;
    margin: 16px 0;
  }
  .top-section .w3-center:after {
    width: 60px;
    height: 3px;
    margin: 0 auto;
  }
  .backend-section {
    padding: 100px 0 70px;
  }
  .backend-section .circle1 {
    top: 37%;
    right: 80%;
    width: 400px;
    height: 400px;
  }
  .backend-section .circle2 {
    top: 10%;
    right: 0%;
  }
  .backend-section .circle2 img {
    width: 221px;
    height: 488px;
  }
  .backend-section h2 {
    font-size: 30px;
    line-height: 42px;
    margin-bottom: 30px;
  }
  .backend-section h2:after {
    width: 60px;
    height: 3px;
    margin: 24px auto 0;
  }
  .backend-section p {
    font-size: 16px;
    line-height: 22px;
    text-align: left;
    padding: 0 60px;
  }
  .backend-section .pic {
    position: relative;
    width: 650px;
    height: 450px;
    object-fit: contain;
    -o-object-fit: contain;
    margin-top: 90px;
  }
  .backend-section .w3-row-padding .w3-col {
    padding: 0 55px;
    height: 265px;
    margin-bottom: 50px;
  }
  .backend-section .w3-row-padding .w3-col h3 {
    font-size: 18px;
    text-align:center!important; 
    line-height: 25px;
    margin-top: 24px;
  }
  .backend-section .w3-row-padding .w3-col p,
  .backend-section .w3-row-padding .w3-col ul {
    font-size: 14px;
    line-height: 20px;
  }
}

/*color-------------------------------------------------------------------------------------*/
/*---------------------------------------------------
    LESS Elements 0.9
  ---------------------------------------------------
    A set of useful LESS mixins
    More info at: http://lesselements.com
  ---------------------------------------------------*/
@media screen and (max-width: 600px) {
  .top-section {
    height: 86vh;
  }
  .top-section .w3-center {
    text-align: left !important;
  }
  .top-section .w3-center img {
    width: 90px;
    height: 40px;
  }
  .top-section .w3-center h2 {
    font-size: 15px;
    line-height: 21px;
  }
  .top-section .w3-center h1 {
    font-size: 36px;
    line-height: 50px;
    margin: 16px 0;
  }
  .top-section .w3-center:after {
    width: 60px;
    height: 3px;
    margin: 0;
  }
  .backend-section {
    padding: 100px 0 110px;
  }
  .backend-section .circle1 {
    top: 21%;
    right: 70%;
    width: 299px;
    height: 299px;
  }
  .backend-section .circle2 {
    top: 10%;
    right: 0%;
  }
  .backend-section .circle2 img {
    width: 154px;
    height: 391px;
  }
  .backend-section h2 {
    font-size: 30px;
    line-height: 42px;
    margin-bottom: 30px;
    text-align: left;
  }
  .backend-section h2:after {
    width: 60px;
    height: 3px;
    margin: 16px 0 0;
  }
  .backend-section p {
    margin-bottom: 20px;
    padding: 0 24px;
    font-size: 16px;
  }
  .backend-section .pic {
    position: relative;
    width: 95%;
    height: auto;
    object-fit: contain;
    -o-object-fit: contain;
    margin-top: 172px;
  }
  .backend-section .w3-row-padding .w3-col {
    height: initial;
    padding: 0 24px;
  }
  .backend-section .w3-row-padding .w3-col .icon {
    margin: 0;
  }
  .backend-section .w3-row-padding .w3-col h3 {
    text-align: justify !important;
    margin: 30px 0px 30px 0px;
    text-align: left;
  }
  .backend-section .w3-row-padding .w3-col p,
  .backend-section .w3-row-padding .w3-col ul {
    max-width: 500px;
  }
}